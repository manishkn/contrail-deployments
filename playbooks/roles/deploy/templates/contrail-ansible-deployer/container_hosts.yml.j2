CONTAINER_REGISTRY: {{ contrailregistry }}
REGISTRY_PRIVATE_SECURE: false
REGISTRY_PRIVATE_INSECURE: false
contrail_configuration:
  LINUX_DISTR: {{ os }}
  OPENSTACK_VERSION: {{ sku }}
  CONTRAIL_VERSION: {{ version }}
  CONTROLLER_NODES: 192.16.7.13,192.16.7.14,192.16.7.15
{% if orchestrator == 'openstack' %}
  AUTH_MODE: keystone
  KEYSTONE_AUTH_ADMIN_PASSWORD: {{ admin_password }}
  KEYSTONE_AUTH_HOST: "{{ ansible_all_ipv4_addresses[0] }}"
  RABBITMQ_PORT: 5673
{% endif %}
  PHYSICAL_INTERFACE: bond0
  VROUTER_GATEWAY: 192.168.7.1

{% if roles is defined %}
roles: 
{% for key , value in roles.iteritems() %}
  {{ key }}:
    {% for item in value %} {{ item }}:
    {% endfor %}
{% endfor %}
{% endif %}
